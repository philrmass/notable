{
  "private": true,
  "name": "notable",
  "version": "1.1.2",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact build --no-prerender",
    "deploy": "gh-pages -d build",
    "dev": "DEV=true cross-env NODE_OPTIONS=--openssl-legacy-provider preact watch --clear=false",
    "eslint": "eslint 'src/**'",
    "lint": "yarn eslint && yarn stylelint",
    "predeploy": "yarn build",
    "serve": "sirv build --port 8080 --cors --single",
    "stylelint": "stylelint '**/*.css'"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-preact": "^1.1.3",
    "preact-cli": "^3.0.0",
    "sirv-cli": "^1.0.3",
    "stylelint": "^15.6.1",
    "stylelint-config-standard": "^33.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "gh-pages": "^5.0.0",
    "history": "^5.3.0",
    "jest": "^29.5.0",
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2",
    "preact-router": "^4.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-scrolling": "^1.3.3",
    "react-dnd-touch-backend": "^16.0.1",
    "utilities": "https://github.com/philrmass/utilities#v0.10.0",
    "uuid": "^9.0.0"
  }
}
